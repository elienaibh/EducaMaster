{
  "name": "educamaster",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "check-deps": "ts-node --project scripts/tsconfig.json scripts/check-dependencies.ts",
    "check-next": "ts-node --project scripts/tsconfig.json scripts/check-next-errors.ts",
    "check-all": "npm run lint && npm run type-check && npm run check-deps && npm run check-next",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "seed": "ts-node -O {\\\"module\\\":\\\"CommonJS\\\"} prisma/seed.ts",
    "check-errors": "ts-node -O {\\\"module\\\":\\\"CommonJS\\\"} scripts/check-errors.ts",
    "fix:types": "ts-node --project scripts/tsconfig.json scripts/fix-types.ts",
    "fix:unused": "ts-node --project scripts/tsconfig.json scripts/fix-unused.ts",
    "fix:non-null": "ts-node --project scripts/tsconfig.json scripts/fix-non-null.ts",
    "fix:format": "ts-node --project scripts/tsconfig.json scripts/fix-formatting.ts",
    "fix:all": "npm run fix:format && npm run fix:types && npm run fix:unused && npm run fix:non-null && npm run lint",
    "predev": "npm run check-all"
  },
  "prisma": {
    "seed": "ts-node -O {\\\"module\\\":\\\"CommonJS\\\"} prisma/seed.ts"
  },
